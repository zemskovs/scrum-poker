{
  "name": "simple-scrum-poker",
  "private": true,
  "version": "1.0.0",
  "description": "Simple Scrum Poker",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./build/webpack.config.js",
    "build": "cross-env NODE_ENV=production node ./build/build.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production node ./build/build.js && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production node ./build/build.js && npx cap copy android",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@capacitor/android": "^2.4.6",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "^2.4.6",
    "dom7": "^3.0.0",
    "framework7": "^6.0.11",
    "framework7-icons": "^4.0.2",
    "framework7-react": "^6.0.11",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "skeleton-elements": "^3.3.0",
    "swiper": "^6.4.15"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.8",
    "@capacitor/cli": "^2.4.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "cordova-res": "^0.15.3",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "ora": "^5.3.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-refresh": "^0.9.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
